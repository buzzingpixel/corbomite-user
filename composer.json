{
    "name": "buzzingpixel/corbomite-user",
    "version": "3.0.1",
    "license": "Apache-2.0",
    "description": "Corbomite User",
    "authors": [
        {
            "name": "TJ Draper",
            "email": "tj@buzzingpixel.com",
            "homepage": "https://buzzingpixel.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "ext-pdo": "*",
        "buzzingpixel/cookie-api": "^1.1",
        "buzzingpixel/corbomite-db": "^1.2.1",
        "buzzingpixel/corbomite-di": "^1.0",
        "buzzingpixel/corbomite-events": "^2.0",
        "buzzingpixel/corbomite-flash-data": "^1.0.1",
        "buzzingpixel/corbomite-migrations": "^1.0",
        "buzzingpixel/request-data-store": "^1.0.1",
        "ramsey/uuid": "^3.8",
        "zendframework/zend-diactoros": "^1.8 || ^2.0.0"
    },
    "require-dev": {
        "buzzingpixel/corbomite-schedule": "^1.0.1",
        "codedungeon/phpunit-result-printer": "^0.26.2",
        "doctrine/coding-standard": "^6.0",
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpmd/phpmd": "^2.6",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.4",
        "twig/twig": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "corbomite\\user\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "corbomite\\tests\\": "tests/"
        }
    },
    "config": {
        "bin-compat": "full",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "diConfigFilePath": "src/diConfig.php",
        "cliActionConfigFilePath": "src/cliActionConfig.php",
        "scheduleConfigFilePath": "src/scheduleConfig.php",
        "httpActionConfigFilePath": "src/httpActionConfig.php",
        "twigExtensions": [
            "corbomite\\user\\twigextensions\\UserTwigExtension"
        ]
    }
}
